{
  "name": "google-ads-tracker",
  "vendor": "vtex",
  "version": "1.0.0",
  "title": "Google Ads Tracker",
  "description": "Google Ads Tracker",
  "mustUpdateAt": "2019-04-03",
  "scripts": {
    "prereleasy": "bash lint.sh",
    "postreleasy": "vtex publish --verbose"
  },
  "builders": {
    "react": "3.x"
  },
  "dependencies": {
    "vtex.pixel-manager": "0.x"
  },
  "policies": [
    {
      "name": "pixel"
    }
  ],
  "settingsSchema": {
    "title": "Google Ads Tracker",
    "type": "object",
    "properties": {
      "conversionId": {
        "title": "Conversion ID",
        "type": "string",
        "description": "Enter the Conversion ID (AW-XXXX) from your Google Ads"
      },
      "conversionLabel": {
        "title": "Conversion Label",
        "type": "string",
        "description": "Enter the Conversion Label from your Google Ads"
      }
    },
    "required": [
      "conversionId"
    ]
  }
}
